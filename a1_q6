import java.util.*;
public class revnum {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        System.out.print("enetr any number: ");
        int n = scn.nextInt();
        System.out.print("number reversed: ");
        while(n != 0){
            int dig = n % 10;
            n = n / 10;
            System.out.print(dig);
        }


    }
    

}
